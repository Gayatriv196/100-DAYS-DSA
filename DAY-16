****LEETCODE Problem #26****
class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int n = nums.size();
        
        int x = 0; // index of the last unique element
        for (int i = 1; i < n; i++) {
            if (nums[i] > nums[x]) {
                x++;
                nums[x] = nums[i];
            }
        }
        return x + 1; // number of unique elements
    }
};
