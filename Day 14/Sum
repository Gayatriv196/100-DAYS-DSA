#include <iostream>
using namespace std;

int sumArray(int arr[5],int size){
    int sum =0;

    for(int i = 0; i < size; i++) {
       
        sum =sum + arr[i];
    }
    return sum;
}

int main() {
    int arr[]={10,20,60,-20,30};
    int size=5 ;

    cout << "Sum of the array is: " << sumArray(arr, size) << endl;

    return 0;
}
